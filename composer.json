{
    "name": "ohdearapp/laravel-ohdear",
    "description": "Easily integrate the Oh Dear API into a Laravel app",
    "keywords": [
        "ohdearapp",
        "laravel-ohdear"
    ],
    "homepage": "https://github.com/ohdearapp/laravel-ohdear",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://ohdear.app",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0",
        "ohdearapp/ohdear-php-sdk": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "OhDear\\LaravelOhDear\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OhDear\\LaravelOhDear\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OhDear\\LaravelOhDear\\LaravelOhDearServiceProvider"
            ],
            "aliases": {
                "LaravelOhDear": "OhDear\\LaravelOhDear\\LaravelOhDearFacade"
            }
        }
    }
}
